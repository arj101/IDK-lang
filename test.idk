let a = [10,20,30,40]
let b = [50,60 ,70, 80]


//[[10, 50], 10] -> [10, 50, 10]
fun flatten(array) {
    for let i = 0; i < length(array); i = i + 1 {
        let newElt = [0, 0, 0]
        newElt.0 = array.(i).(0).0
        newElt.1 = array.(i).(0).1
        newElt.2 = array.(i).1
        array.(i) = newElt
    }
    array
}

println(flatten(a * b * a))

